   "description": "Primer proyecto Node js",
   "main": "index.html",
   "scripts": {
      "dev": "lite-server",
      "start": "concurrently  \"npm run watch:scss\" \"npm run dev\"",
      "test": "echo \"Error: no test specified\" && exit 1",
      "scss": "node-sass -o static/css/ static/css/",
      "watch:scss": "onchange \"static/css/*.scss\" -- npm run scss",
      "clean": "rimraf dist",
      "imagemin": "imagemin static/images/* --out-dir=dist/images",
      "usemin": "usemin src/index.html -d dist -o dist/index.html && usemin src/about.html -d dist -o dist/about.html && usemin src/contactos.html -d dist -o dist/contactos.html && usemin src/precios.html -d dist -o dist/precios.html",
      "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin && npm run copyfonts"
   },
